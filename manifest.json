{
  "name": "Multitran Search",
  "description": "Provides you the ability to use multitran without visiting it's webpage",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service-worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "",
      "32": "",
      "48": "",
      "128": ""
    }
  },
  "icons": {
    "16": "",
    "32": "",
    "48": "",
    "128": ""
  }
}